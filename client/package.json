{
  "name": "study-tracker-client",
  "version": "1.0.0",
  "description": "React front-end client for Study Tracker",
  "private": true,
  "author": "Will Oemler <will@oemler.io>",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build:less": "less-watch-compiler --run-once src/less/ build/static/css/ app.less",
    "dev": "vite",
    "build:js": "vite build",
    "lint": "eslint src --ext .js,.jsx --fix",
    "build": "CI=false && npm-run-all build:js build:less",
    "preview": "vite preview",
    "test": "vitest run"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "browserslist": [
    "> 0.5%",
    "last 2 major versions",
    "not dead",
    "Chrome >= 60",
    "Firefox >= 60",
    "not Edge < 79",
    "Firefox ESR",
    "iOS >= 10",
    "Safari >= 10",
    "Android >= 6",
    "not Explorer <= 11"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-brands-svg-icons": "6.4.0",
    "@fortawesome/free-regular-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@reduxjs/toolkit": "1.9.5",
    "@sweetalert2/theme-bootstrap-4": "^5.0.15",
    "@tanstack/react-query": "^5.75.0",
    "@tanstack/react-table": "^8.20.5",
    "axios": "1.8.3",
    "bootstrap": "5.3.0",
    "crossfilter2": "^1.5.4",
    "dateformat": "^5.0.3",
    "formik": "2.4.1",
    "history": "5.3.0",
    "i18next": "22.5.1",
    "jsonwebtoken": "9.0.0",
    "jwt-decode": "3.1.2",
    "moment": "2.29.4",
    "notyf": "3.10.0",
    "prop-types": "^15.8.1",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-app-polyfill": "2.0.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-bootstrap": "2.7.4",
    "react-datepicker": "^7.5.0",
    "react-datetime": "3.2.0",
    "react-dom": "^18.3.1",
    "react-dragula": "1.1.17",
    "react-dropzone": "^14.2.3",
    "react-feather": "2.0.10",
    "react-helmet-async": "1.3.0",
    "react-i18next": "12.3.1",
    "react-input-mask": "2.0.4",
    "react-perfect-scrollbar": "1.5.8",
    "react-popper": "2.3.0",
    "react-quill": "2.0.0",
    "react-redux": "8.0.7",
    "react-router-dom": "6.12.0",
    "react-select": "^5.10.1",
    "react-syntax-highlighter": "15.5.0",
    "react-transition-group": "^4.4.2",
    "sanitize-html": "^2.12.1",
    "sweetalert2": "11.14.2",
    "web-vitals": "^2.1.4",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "less": "^4.2.0",
    "less-watch-compiler": "^1.16.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.31",
    "postcss-cli": "9.1.0",
    "prettier": "^3.3.3",
    "sass": "1.51.0",
    "vite": "^5.1.4",
    "vitest": "^1.2.2"
  }
}
